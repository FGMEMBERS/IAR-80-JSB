<!--PROP WASH-->
	<model>
		<path>Aircraft/IAR80/Models/Fx/prop-wash.xml</path>
		<offsets>
			<x-m>10</x-m>
			<y-m> 0</y-m>
			<z-m> 0</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>
				<property>orientation/pitch-deg</property>
				<factor>-1.0</factor>
			</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>

	<animation>
		<type>rotate</type>
		<object-name>prop-wash</object-name>
		<property>orientation/pitch-deg</property>
		<factor>-1.0</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>prop-wash</object-name>
		<property>position/altitude-agl-ft</property>
		<factor>-0.3</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y> 0</y>
			<z> 0</z>
		</axis>
	</animation>

	<animation>
