<?xml version="1.0" encoding="utf-8"?>
<!--IAR80 Model animation file
	Datum is tip of propeller spinner-->

<PropertyList>

	<path>iar_80c.ac</path>

<!-- LIVERY SCRIPT -->
	<nasal>
		<load>
			var livery_update = aircraft.livery_update.new("Aircraft/IAR80/Models/Liveries", 10);
		</load>
		<unload>
			livery_update.stop();
		</unload>
	</nasal>

<!-- Collision Ignore for propeller -->
	<animation>
		<object-name>Propeller</object-name>
