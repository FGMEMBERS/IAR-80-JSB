<!-- PARTICLE EFFECTS -->
	<model>
		<path>Aircraft/IAR80/Models/fx.xml</path>
	</model>

<!-- COCKPIT -->
	<model>
		<path>Aircraft/IAR80/Models/cockpit.xml</path>
