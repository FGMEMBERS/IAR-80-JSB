<!-- COWL FLAPS -->
	<animation>
		<type>select</type>
		<object-name>CowlFlapsO</object-name>
		<condition>
			<greater-than>
				<property>sim/multiplay/generic/float[1]</property>
				<value>0.8</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps4</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.8</value>
				</less-than-equals>
				<greater-than>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.6</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps3</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.6</value>
				</less-than-equals>
				<greater-than>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.4</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps2</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.4</value>
				</less-than-equals>
				<greater-than>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.2</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps1</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.2</value>
				</less-than-equals>
				<greater-than>
					<property>sim/multiplay/generic/float[1]</property>
					<value>0.1</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlapsC</object-name>
		<condition>
			<less-than-equals>
				<property>sim/multiplay/generic/float[1]</property>
				<value>0.1</value>
			</less-than-equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>01_WindShieldDrops</object-name>
		<condition>
		    <or>
			<greater-than>
			    <property>environment/rain-norm</property>
			    <value>0</value>
			</greater-than>
			<greater-than>
			    <property>environment/snow-norm</property>
			    <value>0</value>
			</greater-than>
		    </or>
		</condition>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>01_WindShieldDrops</object-name>
		<property>velocities/airspeed-kt</property>
		<!-- <property>/sim/time/elapsed-sec</property> -->
		<factor>0.01</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>



