						<property>controls/canopy/position-norm</property>
						<value>0.1</value>
					</less-than>
				</and>
			</condition>
			<volume>0.5</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00133</factor>
				<offset>0.0</offset>
				<min>0.75</min>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<reference-dist>1</reference-dist>
			<max-dist>100</max-dist>
		</engine-lo>

		<engine-hi>
			<name>engine-hi</name>
			<mode>looped</mode>
			<path>Aircraft/IAR80/Sounds/engine_hi_k14e.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<greater-than>
					<property>/engines/engine[0]/rpm</property>
					<value>1499</value>
				</greater-than>
				<not>
					<and>
						<property>/sim/current-view/internal</property>
						<less-than>
							<property>controls/canopy/position-norm</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<volume>
				<property>/engines/engine[0]/snd-vol</property>
				<factor>1</factor>
				<offset>0</offset>
				<min>0.1</min>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00066</factor>
				<offset>0.0</offset>
				<min>1.0</min>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<orientation>
				<x>1</x>
				<y>0</y>
				<z>0</z>
				<inner-angle>45</inner-angle>
				<outer-angle>120</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<reference-dist>5</reference-dist>
