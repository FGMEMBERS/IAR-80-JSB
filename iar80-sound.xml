<?xml version="1.0"?>
<PropertyList>
	<fx>

	<!-- ENGINE START -->
		<crank>
			<name>crank</name>
			<path>Aircraft/IAR80/Sounds/engine_startup_k14c.wav</path>
			<condition>
				<property>/engines/engine[0]/cranking</property>
				<less-than>
					<property>/engines/engine[0]/rpm</property>
					<value>400</value>
				</less-than>
				<not>
					<and>
						<property>/sim/current-view/internal</property>
						<less-than>
							<property>/controls/canopy/position-norm</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<volume>1.0</volume>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<reference-dist>20</reference-dist>
			<max-dist>2000</max-dist>
		</crank>

		<crank>
			<name>crank-in</name>
			<path>Aircraft/IAR80/Sounds/engine_startup_k14c.wav</path>
			<condition>
				<property>/engines/engine[0]/cranking</property>
				<less-than>
					<property>/engines/engine[0]/rpm</property>
					<value>400</value>
				</less-than>
				<and>
					<property>/sim/current-view/internal</property>
					<less-than>
						<property>controls/canopy/position-norm</property>
						<value>0.1</value>
					</less-than>
				</and>
			</condition>
			<volume>0.6</volume>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<reference-dist>1</reference-dist>
			<max-dist>100</max-dist>
		</crank>

	<!-- ENGINE RUNNING -->


 <!-- ENGINE RUNNING-LOAD -->
		<engineload_ext>
			<name>engine-load-ext</name>
			<mode>looped</mode>
			<path>Aircraft/IAR80/Sounds/iar-Slow-d.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<less-than>
					<property>/engines/engine[0]/rpm</property>
					<value>5000</value>
				</less-than>
				<not>
					<and>
						<property>/sim/current-view/internal</property>
						<less-than>
							<property>/controls/canopy/position-norm</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<volume>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00045</factor>
				<offset>-0.6</offset>
				<min>0.1</min>
				<max>1.8</max>
			</volume>
			<volume>
				<property>/engines/engine[0]/mp-osi</property>
				<factor>0.015</factor>
				<offset>0.8</offset>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00069</factor>
				<offset>0.0</offset>
				<min>0.5</min>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<reference-dist>20</reference-dist>
			<max-dist>2000</max-dist>
		</engineload_ext>

		<engineload_ext>
			<name>engine-load-ext_in</name>
			<mode>looped</mode>
			<path>Aircraft/IAR80/Sounds/iar-Slow-d.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<less-than>
					<property>/engines/engine[0]/rpm</property>
					<value>5000</value>
				</less-than>
				<and>
					<property>/sim/current-view/internal</property>
					<less-than>
						<property>controls/canopy/position-norm</property>
						<value>0.1</value>
					</less-than>
				</and>
			</condition>
			<volume>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00035</factor>
				<offset>-0.6</offset>
				<min>0.05</min>
				<max>1.5</max>
			</volume>
			<volume>
				<property>/engines/engine[0]/mp-osi</property>
				<factor>0.015</factor>
				<offset>0.8</offset>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00069</factor>
				<offset>0.0</offset>
				<min>0.5</min>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<reference-dist>1.5</reference-dist>
			<max-dist>150</max-dist>
		</engineload_ext>

	<!--  NEW SOUNDS -->
		<engine-lo>
			<name>engine-idle</name>
			<mode>looped</mode>
			<path>Aircraft/IAR80/Sounds/engine_idle_k14e.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<greater-than>
					<property>/engines/engine[0]/rpm</property>
					<value>100</value>
				</greater-than>
				<less-than>
					<property>/engines/engine[0]/rpm</property>
					<value>1500</value>
				</less-than>
				<not>
					<and>
						<property>/sim/current-view/internal</property>
						<less-than>
							<property>controls/canopy/position-norm</property>
							<value>0.1</value>
						</less-than>
					</and>
				</not>
			</condition>
			<volume>1.0</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00133</factor>
				<offset>0.0</offset>
				<min>0.75</min>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<orientation>
				<x>1</x>
				<y>0</y>
				<z>0</z>
				<inner-angle>45</inner-angle>
				<outer-angle>120</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<reference-dist>5</reference-dist>
			<max-dist>500</max-dist>
		</engine-lo>

		<engine-lo>
			<name>engine-idle_in</name>
			<mode>looped</mode>
			<path>Aircraft/IAR80/Sounds/engine_idle_k14e.wav</path>
			<condition>
