		<engine-hi>
			<name>engine-hi_in</name>
			<mode>looped</mode>
			<path>Aircraft/IAR80/Sounds/engine_hi_k14e.wav</path>
			<condition>
				<property>/engines/engine[0]/running</property>
				<greater-than>
					<property>/engines/engine[0]/rpm</property>
					<value>1499</value>
				</greater-than>
				<and>
					<property>/sim/current-view/internal</property>
					<less-than>
						<property>controls/canopy/position-norm</property>
						<value>0.1</value>
					</less-than>
				</and>
			</condition>
			<volume>
				<property>/engines/engine[0]/snd-vol</property>
				<factor>0.5</factor>
				<offset>0</offset>
				<min>0.1</min>
				<max>0.7</max>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00066</factor>
				<offset>0.0</offset>
				<min>1.0</min>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
			<reference-dist>1</reference-dist>
			<max-dist>100</max-dist>
		</engine-hi>


	<!-- ENGINE STOP -->
		<enginestop>
			<name>engine_off</name>
			<path>Aircraft/IAR80/Sounds/shutdown.wav</path>
			<type>raise</type>
			<condition>
				<and>
					<greater-than>
						<property>/engines/engine[0]/rpm</property>
						<value>50</value>
					</greater-than>
					<not>
						<property>/engines/engine[0]/running</property>
					</not>
					<not>
						<property>/engines/engine[0]/cranking</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.03</factor>
				<offset>-0.20</offset>
				<min>0.25</min>
				<max>1.5</max>
			</volume>
			<volume>
				<property>/sim/current-view/internal</property>
				<offset>0.65</offset>
				<factor>-0.2</factor>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00066</factor>
				<min>0.0</min>
				<max>1.5</max>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
		</enginestop>

	<!-- ENGINE COUGH -->
		<enginecough>
			<name>engine_cough</name>
			<path>Aircraft/IAR80/Sounds/coughing.wav</path>
			<type>fall</type>
			<condition>
				<and>
					<greater-than>
						<property>/engines/engine[0]/rpm</property>
						<value>300</value>
					</greater-than>
					<not>
						<property>/engines/engine[0]/running</property>
					</not>
					<not>
						<property>/engines/engine[0]/cranking</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00066</factor>
				<offset>-0.20</offset>
				<min>0.15</min>
				<max>1.5</max>
			</volume>
			<volume>
				<property>/sim/current-view/internal</property>
				<offset>0.65</offset>
				<factor>-0.2</factor>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00000025</factor>
				<min>0.0</min>
				<max>1.5</max>
			</pitch>
			<position>
				<x-m> 0.96</x-m>
				<y-m> 0.00</y-m>
				<z-m> 0.00</z-m>
			</position>
		</enginecough>

		<!-- GEAR RUMBLE -->
		<rumbleL>
			<name>rumble-left</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<and>
					<property>gear/gear[0]/wow</property>
					<greater-than>
						<property>gear/gear[0]/rollspeed-ms</property>
						<value>1</value>
					</greater-than>
				</and>
			</condition>
			<volume>
				<property>gear/gear[0]/rollspeed-ms</property>
				<type>log</type>
				<factor>0.15</factor>
			</volume>
			<pitch>
				<property>gear/gear[0]/rollspeed-ms</property>
				<type>log</type>
				<factor>0.70</factor>
			</pitch>
			<position>
				<x-m> 2.573</x-m>
				<y-m>-1.637</y-m>
				<z-m>-1.595</z-m>
			</position>
			<reference-dist>2.5</reference-dist>
			<max-dist>20</max-dist>
		</rumbleL>

		<rumbleR>
			<name>rumble-right</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
